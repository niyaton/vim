set wildmenu		"imporve command completion
set wildchar=<tab>	"key to start command completion
set wildmode=list:full	" as list and full match
set history=10000	"number of history for completion

" like as history completion of zsh
cnoremap <C-p> <Up>
cnoremap <Up>  <C-p>
cnoremap <C-n> <Down>
cnoremap <Down>  <C-n>

" Use neocomplcache
source ~/.vim/rc.d/vimrc.completion.neocomplcache
